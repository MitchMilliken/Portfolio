<table id="animals">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Color</th>
            <th>Age</th>
            <th>Extinct</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var animal in Model.Animals)
        {
            <tr>
                <td>@animal.Id</td>
                <td>@animal.Name</td>
                <td>@animal.Color</td>
                <td>@animal.Age</td>
                <td>@animal.Extinct</td>
            </tr>
        }
    </tbody>
</table>